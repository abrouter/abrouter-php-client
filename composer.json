{
    "name": "abrouter/abrouter-php-client",
    "type": "library",
    "description": "AbrPHPClient :construction_worker_woman: is a PHP library to run ab-tests via ABRouter.",
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Abrouter\\Client\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Yatsenko",
            "email": "abrouter@proxiedmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "php-di/php-di": "^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Abrouter\\Client\\Tests\\": "tests/"
        }
    }
}
